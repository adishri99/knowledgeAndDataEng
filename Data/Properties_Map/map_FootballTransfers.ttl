@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dbpedia: <https://dbpedia.org/ontology/>.
@prefix dbpediaProp: <https://dbpedia.org/property/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.

<#FootballTransfers2021Map>
	a rr:TriplesMap ;
	rr:logicalTable [
		rr:tableName "FOOTBALLTRANSFERS2021"
	] ;
	rr:subjectMap [
		rr:template "http://ex.org/entity/{PLAYER_NAME}" ;
		rr:class dbpedia:SoccerPlayer;  
	] ;
	rr:predicateObjectMap [
		rr:predicate dbpediaProp:currentClub ;
		rr:objectMap [ 
			rr:column "CLUB_NAME";
			rr:termType rr:Literal;
			rr:class dbpedia:SoccerClub;
			 ];
	] ;
	rr:predicateObjectMap [
		rr:predicate foaf:age ;
		rr:objectMap [ 
			rr:column "AGE";
			rr:termType rr:Literal;
			rr:class foaf:Person;
			rr:datatype xsd:int;
			 ];
	] ;
	rr:predicateObjectMap [
		rr:predicate dbpediaProp:playsAtPosition ;
		rr:objectMap [ 
			rr:column "POSITION";
			rr:termType rr:Literal;
			rr:class dbpedia:SoccerPlayer;
			 ];
	] ;
	rr:predicateObjectMap [
		rr:predicate dbpediaProp:transferedFrom ;
		rr:objectMap [ 
			rr:column "CLUB_INVOLVED_NAME";
			rr:termType rr:Literal;
			rr:class dbpedia:SoccerClub;
			 ];
	] ;
	rr:predicateObjectMap [
		rr:predicate dbpediaProp:transferFee ;
		rr:objectMap [ 
			rr:column "FEE_CLEANED";
			rr:termType rr:Literal;
			rr:class dbpedia:SoccerClub;
			 ];
	] ;
	.