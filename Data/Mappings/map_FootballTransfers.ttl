	] ;
	rr:predicateObjectMap [
		rr:predicate dbpediaProp:transferSeason ;
		rr:objectMap [ 
			rr:column "SEASON";
			rr:termType rr:Literal;
			rr:class dbpedia:SoccerLeagueSeason;
